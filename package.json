  },
  "dependencies": {
    "@webb/console": "^2020.3.29",
    "app-root-path": "^3.0.0",
    "env-smart": "^2.1.2"
  },
  "description": "A smarter .env",
  "devDependencies": {
    "@inc/eslint-config": "^2019.10.22",
    "@webb/config-nyc": "^2020.3.29",
    "@webb/config-prettier": "^2020.3.29",
    "@webb/test": "^2020.3.29",
    "chronver": "^2020.3.9",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "prettier": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "updates": "^10.2.6"
  },
  "homepage": "https://code.webb.page/vne",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run pretty && npm run increment && git add -A :/"
    }
  },
  "keywords": [
    "conf",
    "config",
    "configuration",
    "env",
    "environment",
    "parse",
    "settings"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "vne",
  "prettier": "@webb/config-prettier",
  "repository": {
    "type": "git",
    "url": "https://code.webb.page/vne"
  },
  "scripts": {
    "build": "npm run cleanup && tsc",
    "cleanup": "del './dist'",
    "increment": "chronver --increment package",
    "pretty": "prettier --write 'dist/**/*.js' 'dist/**/*.ts'",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:typescript": "tsc",
    "test:assert": "ts-node test/index.ts",
    "test:coverage": "nyc npm run test:assert"
  },
  "types": "dist/index.d.ts",
  "version": "2020.04.05"
}
